package class_2;

public class Fruit {

	private String pum;
	private int jan, feb, mar, tot, sumJan, sumFeb,sumMar;
	
	public Fruit(String pum, int jan, int feb, int mar) {
		this.pum = pum;
		this.jan = jan;
		this.feb = feb;
		this.mar = mar;
	}
	
	public void calcTot() {
		tot = jan + feb + mar;
	}
	
	public void display() {
		
	}
	
	public static void output() {
		System.out.println("------------------------------");
		System.out.println("PUM\tJAN\tFEB\tMAR\tTOT");
		System.out.println("------------------------------");
		System.out.println("------------------------------");
	}
}
