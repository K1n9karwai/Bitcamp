package abstract_1;

import java.util.Scanner;

public class CalendarTest {

	private int year;
	private int month;
	public CalendarTest() {

		Scanner scan = new Scanner(System.in);

		System.out.print("년도 입력 : ");
		int year = scan.nextInt();
		System.out.print("월 입력 : ");
		int month = scan.nextInt();

	}

}
