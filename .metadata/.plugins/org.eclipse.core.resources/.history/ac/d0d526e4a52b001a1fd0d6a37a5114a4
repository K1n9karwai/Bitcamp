package inheritance;

public class SalaryManager {

	public SalaryManager() {
		System.out.println("1. 등록 ");
		System.out.println("2. 출력 ");
		System.out.println("3. 수정 ");
		System.out.println("4. 검색 ");
		System.out.println("5. 종료 ");
		
	}
	
	
	public void list() {
		System.out.println(name + "\t" + position + "\t" +
							basepay + "\t" + benefit + "\t" +
							taxRate + "\t" +  salary);
	}
}
